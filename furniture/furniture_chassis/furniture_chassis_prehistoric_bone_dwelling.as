var item_inited:Boolean;var item_ran:Boolean;var house_mc:MovieClip;var body: MovieClip;var door:MovieClip;var frontbones:MovieClip;var skull:MovieClip;var stair:MovieClip;var config_options:Object;var config_option_names:Object;itemInit();function itemInit():void {	if (item_inited) return;	animatee = this;	animated_mcs = [];	animations = ['1'];	loopers = [];	myAnim = '1';        config_option_names = {                body: 'Siding',				door: 'Door',                frontbones: 'Front Bones',                skull: 'Skull',                stair: 'Stairs'        };				config_options = {		body: [			'body_one',			'body_two',			'body_three',			'body_four'		],		door: [			'door_one',			'door_two',			'door_three',			'door_four'		],		frontbones: [			'frontbones_one',			'frontbones_two',			'frontbones_three',			'frontbones_four'		],		skull: [			'skull_mammoth',			'skull_rook',			'skull_hogosaurus',			'skull_chicken'		],		stair: [			'stair_mud',			'stair_spinalbone',			'stair_woodplanks',			'stair_woodmud',			'stair_brokenbone'		]	}	;	house_mc = maincontainer_mc.house_mc;	body = house_mc.bodyContainer_mc.getChildAt(0) as MovieClip;	door = house_mc.doorContainer_mc.getChildAt(0) as MovieClip;	frontbones = house_mc.frontbonesContainer_mc.getChildAt(0) as MovieClip;	skull = house_mc.skullContainer_mc.getChildAt(0) as MovieClip;	stair = house_mc.stairContainer_mc.getChildAt(0) as MovieClip;		if (true) {		var temp_ob4:Object = {};		var index:int = 0;		for (var k:String in config_options) {			var which:int = index			if (which >= config_options[k].length) which = config_options[k].length-1			trace(k+' '+which+' '+config_options[k][which]);			temp_ob4[k] = config_options[k][which]		}				initializehead(temp_ob4);	}			item_inited = true;}//itemRun()function itemRun():void {	if (item_ran) return;	//do_trace = true	// DEFAULT!		//stopAll();	//trackFrames();	//playAnimation('warriorPose');	//