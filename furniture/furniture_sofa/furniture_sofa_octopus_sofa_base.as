var item_inited:Boolean;var item_ran:Boolean;var octopussofa_mc:MovieClip;var baseTop:MovieClip;var baseBottom:MovieClip;var fabric:MovieClip;var config_options:Object;var config_option_names:Object;itemInit();function itemInit():void {	if (item_inited) return;	animatee = this;	animated_mcs = [];	animations = ['1'];	loopers = [];	myAnim = '1';        config_option_names = {                base: 'Base',                fabric: 'Fabric'        }				config_options = {		base: [			'purple',			'black',		],		fabric: [			'fabric_purple',			'fabric_red',			'fabric_blue'		]	}		octopussofa_mc = maincontainer_mc.octopussofa_mc;	baseTop = octopussofa_mc.baseTopContainer_mc.getChildAt(0) as MovieClip;	baseBottom = octopussofa_mc.baseBottomContainer_mc.getChildAt(0) as MovieClip;	fabric = octopussofa_mc.fabricContainer_mc.getChildAt(0) as MovieClip;		if (true) {		var temp_ob4:Object = {};		var index:int = 0;		for (var k:String in config_options) {			var which:int = index			if (which >= config_options[k].length) which = config_options[k].length-1			trace(k+' '+which+' '+config_options[k][which]);			temp_ob4[k] = config_options[k][which]		}				initializehead(temp_ob4);	}			item_inited = true;}//itemRun()function itemRun():void {	if (item_ran) return;	//do_trace = true	// DEFAULT!		//stopAll();	//trackFrames();	//playAnimation('warriorPose');	//playAnimationSeq(['idle']);	item_ran = true;	//LogCoords(animatee, 20)	}function initializehead(ob:Object) {	log(112, 'initializehead '+ob)	if (!ob) return;		baseTop = addArticleToContainer('baseTop_'+ob.base, octopussofa_mc, 'baseTopContainer_mc') || baseTop;	baseBottom = addArticleToContainer('baseBottom_'+ob.base, octopussofa_mc, 'baseBottomContainer_mc') || baseBottom;		fabric = addArticleToContainer(ob.fabric, octopussofa_mc, 'fabricContainer_mc') || fabric;}